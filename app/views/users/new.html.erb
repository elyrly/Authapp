<div class="jumbotron">
  <div class="container">

<h2><span class="glyphicon glyphicon-indent-left"></span> Sign Up</h2>

<%= form_for @user do |x| %>
  <%= x.text_field :username, placeholder: " Username"%><br>
  <%= x.text_field :email, placeholder: " Email" %><br>
  <%= x.password_field :password, placeholder: " Password" %><br>
  <%= x.password_field :password_confirmation, placeholder: " Password Confirmation" %><br>
   <button type="submit" class="btn btn-default btn-lg">Create User</button>
  <%end%>

</div>
</div>

